{
  "manifest_version": 2,
  "name": "WebCustomizer",
  "version": "1.0",
  "description": "Customize your web experience with this awesome extension!",
  "author": "Renbits (Ren)",
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["assets/js/content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
